@model  List<Model.ViewModels.AccountViewModel>
@{
    ViewBag.Title = "Home Page";
}

<div class="nopadding">
    @Html.Partial("SearchPartialView", new Model.ViewModels.GeneralSearchView())
    @Html.Partial("AdsPartialView")
    @Html.Partial("CardsPartialView", @Model)
